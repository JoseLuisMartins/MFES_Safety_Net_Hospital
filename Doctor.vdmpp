class Doctor
instance variables
	private name: Utils`String := [];
	private specialty: Utils`Specialty;
	private appointments: set of Appointment := {};

operations
	--contructor
		public Doctor : Utils`String * Utils`Specialty ==> Doctor
		Doctor(n, s) == (
			name := n;
			specialty := s;
			return self;
		);
		
		public addAppointment : Appointment ==> ()
		addAppointment(a) == (
			appointments := appointments union {a};
		);



functions
-- TODO Define functiones here

end Doctor